{
  "name": "@jarsmohella/lorry",
  "version": "1.0.0",
  "description": "A CLI for interacting with the ShipEngine API",
  "main": "./lib/index.js",
  "bin": {
    "lorry": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarshalJoe/lorry.git"
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "test": "cross-env jest --config jestconfig.json",
    "build": "cross-env tsc",
    "format": "cross-env prettier --write \"src/*.ts\"",
    "lint": "cross-env tslint -p tsconfig.json"
  },
  "author": "Joe Marshall",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.20.0",
    "chalk": "^2.4.1",
    "commander": "^6.1.0",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "netrc-parser": "^3.1.6"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  },
  "files": [
    "lib/**/*"
  ]
